class('java.lang.Integer':
  def('+' | other:
    'java.lang.Math'.addExact(this, other)
  )
  def('-' | other:
    'java.lang.Math'.subtractExact(this, other)
  )
  def('<' | other:
    'java.lang.Integer'.signum(this.compareTo(other)) == -1
  )
  def('-@':
    'java.lang.Math'.negateExact(this)
  )
  def('==' | other:
    this.equals(other)
  )
)

